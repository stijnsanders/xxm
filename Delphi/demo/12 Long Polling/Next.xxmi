[[!var
i:integer;
th,tm,ts,tz:word;
]][[

DecodeTime(Now,th,tm,ts,tz);

<p>>=[FormatDateTime('hh:nn:ss',Now),', ',th,'+',tm,'+',ts,' = ',th+tm+ts]<</p>


i:=Values[0]+1;
if i=10 then
 begin
  <p>Ten events shown</p>
 end
else
  (Context as IXxmContextSuspend).Suspend('demo12',250,1000,'Next.xxmi',i,'Drop.xxmi',i);

]]